programa
{
	
	funcao inicio()
	{
	inteiro num1, num2 
	escreva("========== MAIOR? ==========\n")
	escreva("digite um numero para descobrir qual é maior:")
	leia(num1)
	escreva("digite outro numero para descobrir qual é maior:")
	leia(num2)
	se (num1 >= num2)
	escreva(num1," é maior")
	senao 
	escreva(num2," é maior")
	}
}
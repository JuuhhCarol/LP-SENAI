programa
{
	
	funcao inicio()
	{
		inteiro num1, num2, soma, multiplica
		escreva("digite um numero: ")
		leia(num1)
		escreva("digite o segundo numero: ")
		leia(num2)
		soma=(num1+num2)
		escreva(num1, "+", num2, "=", soma)
		multiplica=(soma*num1)
		escreva("\n",soma,"*",num1,"=",multiplica)
	}
}